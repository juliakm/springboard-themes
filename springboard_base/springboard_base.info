name = Springboard Base
description = A stripped-down, responsive, HTML5 and CSS3 base theme for Springboard sites.
package = Springboard
version = 7.x-4.x-dev
core = 7.x

;Regions
regions[header] = Header
regions[content] = Content
regions[footer] = Footer

;Stylesheets
stylesheets[all][] = css/bootstrap.css
stylesheets[all][] = css/bootstrap-responsive.css
stylesheets[all][] = css/base-styles.css

;Scripts
scripts[] = js/bootstrap.min.js
;scripts[] = js/jquery-ui.min.js

;Theme Settings
settings[default_logo] = 0

