name = Springboard Base
description = A stripped-down, responsive, HTML5 and CSS3 base theme for Springboard sites.
package = Springboard
version = 7.x-4.0.3
core = 7.x

;Regions
regions[header] = Header
regions[content] = Content
regions[footer] = Footer

;Stylesheets
stylesheets[all][] = css/bootstrap.min.css
stylesheets[all][] = css/bootstrap-responsive.min.css
stylesheets[all][] = css/base-styles.css

;Scripts
scripts[] = js/bootstrap.min.js
scripts[] = js/jquery-ui.min.js
